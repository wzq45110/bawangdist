.mainwarn {
  width: 100%;
  height: 100%;
}
.mainwarn .topwarn {
  position: relative;
  width: 100%;
  height: 600rpx;
}
.mainwarn .topwarn .first,
.mainwarn .topwarn .second,
.mainwarn .topwarn .third,
.mainwarn .topwarn .four,
.mainwarn .topwarn .five,
.mainwarn .topwarn .six {
  text-align: center;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.mainwarn .topwarn .first {
  width: 80%;
  height: 60rpx;
  top: 40rpx;
  font-size: 24rpx;
}
.mainwarn .topwarn .second {
  width: 80%;
  height: 50rpx;
  top: 80rpx;
  font-size: 24rpx;
}
.mainwarn .topwarn .third {
  font-size: 32rpx;
  font-weight: 600;
  width: 80%;
  height: 60rpx;
  top: 150rpx;
}
.mainwarn .topwarn .four {
  top: 220rpx;
  width: 240rpx;
  height: 240rpx;
}
.mainwarn .topwarn .four image {
  width: 100%;
  height: 100%;
}
.mainwarn .topwarn .five {
  font-size: 20rpx;
  bottom: 94rpx;
}
.mainwarn .topwarn .six {
  bottom: 0;
  width: 50%;
  height: 80rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.mainwarn .topwarn .six .zi {
  width: 10%;
  height: 60%;
}
.mainwarn .topwarn .six .zi image {
  width: 100%;
  height: 100%;
}
.mainwarn .zhoxiabu {
  width: 100%;
  height: 1000rpx;
  background-color: #eee;
  padding-top: 20rpx;
}
.mainwarn .zhoxiabu .yaoqin {
  width: 100%;
  height: 600rpx;
  background-color: #fff;
}
.mainwarn .zhoxiabu .yaoqin .meyao {
  width: 100%;
  height: 90rpx;
  margin-left: 20rpx;
  line-height: 90rpx;
}
.mainwarn .zhoxiabu .yaoqin .melan {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 150rpx;
  border-top: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
}
.mainwarn .zhoxiabu .yaoqin .melan .melanzi {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  width: 30%;
  height: 60%;
  font-size: 26rpx;
}
.mainwarn .zhoxiabu .yaoqin .melan .melanzi text {
  font-size: 34rpx;
  color: orange;
  font-weight: 600;
}
.mainwarn .zhoxiabu .xaingxi {
  position: relative;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
}
.mainwarn .zhoxiabu .xaingxi .tuxaing {
  left: 20rpx;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.mainwarn .zhoxiabu .xaingxi .tuxaing image {
  width: 100%;
  height: 100%;
}
.mainwarn .zhoxiabu .xaingxi .neizhi {
  left: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 400rpx;
  height: 70%;
}
.mainwarn .zhoxiabu .xaingxi .babzui {
  right: 20rpx;
  color: chocolate;
  font-weight: 700;
  font-size: 28rpx;
}
.mainwarn .zhoxiabu .xaingxi .tuxaing,
.mainwarn .zhoxiabu .xaingxi .neizhi,
.mainwarn .zhoxiabu .xaingxi .babzui {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.mainwarn .zhoxiabu .jiange {
  width: 100%;
  height: 70rpx;
  background-color: #eee;
}
.mainwarn .zhoxiabu .canyulan {
  width: 100%;
  height: 600rpx;
  background-color: #fff;
}
.mainwarn .zhoxiabu .canyulan .ayfs {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
.mainwarn .zhoxiabu .canyulan .fangshxai {
  display: flex;
  justify-content: space-around;
  width: 100%;
  height: 400rpx;
}
.mainwarn .zhoxiabu .canyulan .fangshxai .zhonger {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 30%;
  height: 400rpx;
  padding-top: 30rpx;
}
.mainwarn .zhoxiabu .canyulan .fangshxai .zhonger .tubiao {
  width: 50%;
  height: 150rpx;
}
.mainwarn .zhoxiabu .canyulan .fangshxai .zhonger .quanzi {
  width: 30%;
  height: 80rpx;
}
.mainwarn .zhoxiabu .canyulan .fangshxai .zhonger .textzi {
  width: 80%;
}
.mainwarn .zhoxiabu .canyulan .fangshxai .zhonger image {
  width: 100%;
  height: 100%;
}
