.wenzih {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 200rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.wenzih .logoqu {
  background-color: #ff7900;
  color: #fff;
  font-size: 28rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  width: 180rpx;
  border-radius: 30rpx;
  margin-right: 35rpx;
}
.wenzih .leftjia {
  width: 300rpx;
  height: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
  margin-left: 30rpx;
}
.wenzih .leftjia .tufendan {
  width: 80rpx;
  height: 80rpx;
  border: 10rpx solid #fed6b1;
  border-radius: 50%;
}
.wenzih .leftjia .tufendan image {
  width: 100%;
  height: 100%;
}
.gegetiao {
  width: 100%;
  height: 30rpx;
  background-color: #eeeeee;
}
.dianrei {
  position: relative;
  width: 100%;
  height: 200rpx;
  border-bottom: 1rpx solid #eee;
}
.dianrei .zuodinzhi,
.dianrei .lineba,
.dianrei .youbilo {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.dianrei .zuodinzhi {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  left: 30rpx;
  height: 150rpx;
  width: 60%;
}
.dianrei .lineba {
  width: 1rpx;
  height: 100rpx;
  background-color: #ccc;
  right: 220rpx;
}
.dianrei .youbilo {
  right: 70rpx;
}
