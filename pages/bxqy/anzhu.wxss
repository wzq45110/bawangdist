.anzhuumain {
  width: 100%;
  height: 100%;
  background-color: #eeeeee;
}
.anzhuumain .anquuu {
  position: relative;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  padding-top: 40rpx;
}
.anzhuumain .anquuu .butjiao {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  width: 90%;
  color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  background-color: #ff7900;
}
.anzhuumain .phoneren {
  margin-top: 20rpx;
  background-color: #fff;
  width: 100%;
  padding-top: 30rpx;
  padding-left: 20rpx;
}
.anzhuumain .phoneren .xiaodianh {
  font-size: 24rpx;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
}
