.guanzhumain {
  width: 100%;
  height: 100%;
}
.guanzhumain .shangguan {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: start;
  align-items: center;
  padding-left: 20rpx;
}
.guanzhumain .shangguan .tulogo {
  border-radius: 50%;
  border: 10rpx solid #fed6b3;
  width: 120rpx;
  height: 120rpx;
  margin-left: 30rpx;
}
.guanzhumain .shangguan .tulogo image {
  width: 100%;
  height: 100%;
}
.guanzhumain .crsadmain {
  width: 100%;
  height: 600rpx;
  background-color: #eee;
  padding-top: 20rpx;
}
.guanzhumain .crsadmain .neicar {
  width: 100%;
  height: 100%;
  background-color: #fff;
  padding-top: 10rpx;
}
.guanzhumain .crsadmain .neicar .wepjiang {
  margin-top: 40rpx;
  border-bottom: 1rpx solid #ccc;
  padding-bottom: 30rpx;
}
.guanzhumain .crsadmain .neicar .pic {
  margin-bottom: 30rpx;
  height: 350rpx;
}
.guanzhumain .crsadmain .neicar .pic image {
  height: 100%;
  width: 750rpx;
}
.guanzhumain .crsadmain .neicar .wenzixia .anniuniu {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #ff7900;
  border-radius: 20rpx;
  color: #fff;
  font-size: 26rpx;
}
.guanzhumain .crsadmain .neicar .wenzixia,
.guanzhumain .crsadmain .neicar .textp {
  padding: 0 35rpx;
}
.guanzhumain .crsadmain .neicar .wenzixia,
.guanzhumain .crsadmain .neicar .textp,
.guanzhumain .crsadmain .neicar .shangbu {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;
}
.guanzhumain .crsadmain .neicar .zanan {
  width: 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background-color: #fff;
  border: 1rpx solid #ff7900;
  color: #ff7900;
  border-radius: 10rpx;
}
.guanzhumain .crsadmain .neicar .tianhong {
  width: 100%;
  height: 600rpx;
  border-bottom: 1rpx solid #ccc;
}
.guanzhumain .crsadmain .neicar .tianhong .shnagtian {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.guanzhumain .crsadmain .neicar .tianhong .shnagtian .lefttulo {
  margin-left: 20rpx;
  width: 120rpx;
  height: 120rpx;
}
.guanzhumain .crsadmain .neicar .tianhong .shnagtian .lefttulo image {
  width: 100%;
  height: 100%;
}
.guanzhumain .crsadmain .neicar .tianhong .tucare {
  position: relative;
  width: 100%;
  height: 600rpx;
}
.guanzhumain .crsadmain .neicar .tianhong .tucare .tucarekk {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 0;
  width: 95%;
  height: 350rpx;
}
.guanzhumain .crsadmain .neicar .tianhong .tucare .tucarekk image {
  width: 100%;
  height: 100%;
}
.guanzhumain .crsadmain .neicar .tianhong .tucare .shuomin {
  position: absolute;
  left: 34rpx;
  top: 254rpx;
  width: 95%;
  height: 50rpx;
  z-index: 100;
  color: #fff;
}
